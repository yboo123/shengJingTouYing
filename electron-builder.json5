{
    "productName": "touYing",
    "appId": "com.dengruicode.www",
    "win": {
        "icon": "electron/resource/images/code.ico",
        "target": [
            {
                "target": "nsis",
                "arch": [
                    "x64"
                ]
            }
        ]
    },
    "nsis": {
        "oneClick": false,
        "perMachine": true,
        "allowToChangeInstallationDirectory": true
