    "files": [
        "dist",
        "dist-electron",
        // "public/*.html"
    ],
    "directories": {
        "output": "build"
    },
    "extraFiles": [
        {
            "from": "public/userConfig.json",
            "to": "."
        },        {
            "from": "public/bj.png",
            "to": "."
        }
    ]
}